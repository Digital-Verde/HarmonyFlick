    {
      "cell_type": "code",
      "source": [
        "# Configuraﾃｧﾃｵes iniciais\n",
        "import google.generativeai as genai\n",
        "\n",
        "from google.colab import userdata\n",
        "chave_api = userdata.get('SECRET_KEY')\n",
        "\n",
        "genai.configure(api_key=chave_api)\n",
        "\n",
        "# Funﾃｧﾃ｣o para obter informaﾃｧﾃｵes sobre a pessoa a ser presenteada\n",
        "def obter_informacoes_pessoa(numero_pessoas):\n",
        "    \"\"\"Obtﾃｩm informaﾃｧﾃｵes sobre as pessoas a serem presenteadas.\"\"\"\n",
        "    pessoas = []\n",
        "    for i in range(numero_pessoas):\n",
        "        print(f\"\\nInformaﾃｧﾃｵes da pessoa {i+1}:\")\n",
        "        idade = input(\"Idade: \")\n",
        "        sexo = input(\"Sexo: \")\n",
        "        interesses = input(\"Interesses e hobbies: \")\n",
        "        estilo = input(\"Estilo (clﾃ｡ssico, moderno, esportivo, etc.): \")\n",
        "        personalidade = input(\"Personalidade (extrovertido, introvertido, criativo, etc.): \")\n",
        "        nao_gosta = input(\"O que ela Nﾃグ gosta: \")\n",
        "        pessoas.append({\n",
        "            \"idade\": idade,\n",
        "            \"sexo\": sexo,\n",
        "            \"interesses\": interesses,\n",
        "            \"estilo\": estilo,\n",
        "            \"personalidade\": personalidade,\n",
        "            \"nao_gosta\": nao_gosta\n",
        "        })\n",
        "    return pessoas\n",
        "\n",
        "# Funﾃｧﾃ｣o para obter informaﾃｧﾃｵes sobre o filme\n",
        "def obter_informacoes_filme():\n",
        "    \"\"\"Obtﾃｩm informaﾃｧﾃｵes sobre as preferﾃｪncias do filme.\"\"\"\n",
        "    genero = input(\"Gﾃｪnero do filme: \")\n",
        "    classificacao_etaria = input(\"Classificaﾃｧﾃ｣o etﾃ｡ria: \")\n",
        "    temas = input(\"Temas de interesse: \")\n",
        "    nao_gosta = input(\"O que vocﾃｪs Nﾃグ gostam em um filme: \")\n",
        "    return genero, classificacao_etaria, temas, nao_gosta\n",
        "\n",
        "# Configuraﾃｧﾃｵes do modelo\n",
        "generation_config = {\n",
        "    \"candidate_count\": 1,\n",
        "    \"temperature\": 0.5,\n",
        "}\n",
        "\n",
        "safety_settings={\n",
        "    'HATE': 'BLOCK_NONE',\n",
        "    'HARASSMENT': 'BLOCK_NONE',\n",
        "    'SEXUAL' : 'BLOCK_NONE',\n",
        "    'DANGEROUS' : 'BLOCK_NONE'\n",
        "}\n",
        "\n",
        "model = genai.GenerativeModel(model_name='gemini-1.0-pro',\n",
        "                              generation_config=generation_config,\n",
        "                              safety_settings=safety_settings,)\n",
        "\n",
        "chat = model.start_chat(history=[])\n",
        "\n",
        "# Loop principal\n",
        "while True:\n",
        "    print(\"\\nOlﾃ｡! Queremos te ajudar a escolher um filme! 沁噂")\n",
        "\n",
        "    numero_pessoas = int(input(\"Para quantas pessoas vocﾃｪ estﾃ｡ escolhendo o filme? \"))\n",
        "    pessoas = obter_informacoes_pessoa(numero_pessoas)\n",
        "    genero, classificacao_etaria, temas, nao_gosta_filme = obter_informacoes_filme()\n",
        "\n",
        "    # Cria o prompt com as informaﾃｧﾃｵes das pessoas e do filme\n",
        "    prompt = f\"Preciso escolher um filme para {numero_pessoas} pessoas. \"\n",
        "    for i, pessoa in enumerate(pessoas):\n",
        "        prompt += f\"A pessoa {i+1} tem {pessoa['idade']} anos, ﾃｩ do sexo {pessoa['sexo']}, gosta de {pessoa['interesses']}, tem um estilo {pessoa['estilo']} e uma personalidade {pessoa['personalidade']}. Ela nﾃ｣o gosta de {pessoa['nao_gosta']}. \"\n",
        "    prompt += f\"Em relaﾃｧﾃ｣o ao filme, estamos buscando um filme do gﾃｪnero {genero}, com classificaﾃｧﾃ｣o etﾃ｡ria {classificacao_etaria}, que explore os temas {temas}. Nﾃ｣o gostamos de filmes com {nao_gosta_filme}. Poderia me indicar um filme que agrade a todos?\"\n",
        "\n",
        "    response = chat.send_message(prompt)\n",
        "    print(\"\\nSugestﾃ｣o de filme:\", response.text, '\\n')\n",
        "\n",
        "    continuar = input(\"Deseja buscar outro filme? (s/n): \")\n",
        "    if continuar.lower() != 's':\n",
        "        break\n",
        "\n",
        "print(\"Obrigado por usar o Chatbot de Filmes! 沚ｿ\")"
      ],
